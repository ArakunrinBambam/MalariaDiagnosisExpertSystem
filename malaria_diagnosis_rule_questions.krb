

no_rain
    use what_to_bring(no_rain_gear)
    when
        questions.is_raining(False)

what_to_bring_raincoat
    use what_to_bring(raincoat)
    when
        questions.is_raining(True)
        questions.is_windy(False)

what_isAge 
    use what_isAge($ans)
    when 
        questions.what_is_age($ans)

